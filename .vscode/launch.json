{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "edge",
      "request": "launch",
      "version": "canary",
      "name": "Launch Microsoft Edge (Chromium) against localhost",
      "url": "localhost:8080",
      "port": 9223,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "edge",
      "request": "attach",
      "name": "Attach Microsoft Edge (Chromium) against localhost",
      "url": "localhost:8080",
      "port": 9223,
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome against localhost",
      "port": 9222,
      "url": "localhost:8080",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
